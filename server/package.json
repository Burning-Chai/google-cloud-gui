{
  "name": "google-cloud-gui",
  "version": "0.3.0",
  "description": "Google Cloud services and emulator management webapp",
  "bin": "server.sh",
  "scripts": {
    "start": "nodemon src/server.js --skip-browser"
  },
  "author": "Gabriel Axel <guznik@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/gabiaxel/google-cloud-gui",
  "keywords": [
