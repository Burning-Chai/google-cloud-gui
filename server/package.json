    "body-parser": "1.19.0",
    "express": "4.17.1",
    "lodash-id": "0.14.0",
    "lowdb": "1.0.0",
    "open": "7.0.0",
    "server": "1.0.21",
    "yargs": "15.0.2"
  },
  "devDependencies": {
    "eslint": "6.7.2",
